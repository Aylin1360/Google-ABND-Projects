<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aylin.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Rock"
            android:label="Rock"
            android:parentActivityName="com.example.aylin.musicapp.MainActivity" />
        <activity android:name=".Jazz"
            android:label="Jazz"
            android:parentActivityName="com.example.aylin.musicapp.MainActivity"/>
        <activity android:name=".Blues"
            android:label="Blues"
            android:parentActivityName="com.example.aylin.musicapp.MainActivity"/>
        <activity android:name=".Pop"
            android:label="Blues"
            android:parentActivityName="com.example.aylin.musicapp.MainActivity">
        </activity>
    </application>

</manifest>